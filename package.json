{
  "name": "openai-realtime-console",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@openai/realtime-api-beta": "github:openai/openai-realtime-api-beta",
    "dotenv": "^16.4.5",
    "ws": "^8.18.0"
  },
  "scripts": {
    "start": "nodemon ./index.js",
    "docker:build": "docker build -t relay_server -f ./Dockerfile .",
    "docker:run": "docker run -p 8081:8081 --env-file .env relay_server",
    "docker:stop": "docker stop relay_server"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "nodemon": "^3.1.7"
  }
}
